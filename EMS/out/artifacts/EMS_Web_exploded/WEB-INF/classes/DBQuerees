create database ems;
use ems;

create table user(
    id VARCHAR(255) PRIMARY KEY,
    name VARCHAR(255),
    address VARCHAR(255),
    email VARCHAR(255),
    password VARCHAR(255)
);

create table employee(
    id varchar(255) primary key,
    name varchar(255),
    address varchar(255),
    email varchar(255),
    contact varchar(255)
);